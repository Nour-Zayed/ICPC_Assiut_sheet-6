   "source": [
    "import math\n",
    "a, b, c = map(int, input().split())\n",
    "\n",
    "if a + b > c and a + c > b and b + c > a:\n",
    "    s = (a + b + c) / 2\n",
    "\n",
    "    area = math.sqrt(s * (s - a) * (s - b) * (s - c))\n",
    "    print(\"Valid\")\n",
    "    print(f\"{area:.6f}\") \n",
    "\n",
    "\n",
    "else:\n",
    "    print(\"Invalid\")\n"
   ]
