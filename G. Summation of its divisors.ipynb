   "source": [
    "import math\n",
    "\n",
    "x = int(input())\n",
    "result = 0\n",
    "\n",
    "for i in range(1, int(math.sqrt(x)) + 1):\n",
    "    if x % i == 0:  \n",
    "        result= i+result\n",
    "        if i != x // i:  \n",
    "            result += x // i\n",
    "\n",
    "print(result)\n"
   ]
