   "source": [
    "import math\n",
    "\n",
    "x = int(input())\n",
    "\n",
    "if x == 0 or x == 1:\n",
    "    lenn = 1\n",
    "else:\n",
    "    digit_sum = sum(math.log10(i) for i in range(2, x + 1))\n",
    "    \n",
    "    lenn = math.floor(digit_sum) + 1\n",
    "\n",
    "print(f\"Number of digits of {x}! is {lenn}\")\n"
   ]
