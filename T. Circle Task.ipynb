    "import math\n",
    "\n",
    "X, Y, R, N = map(int, input().split())\n",
    "\n",
    "for _ in range(N):\n",
    "    a, b = map(int, input().split())\n",
    "    distance = math.sqrt((a - X) ** 2 + (b - Y) ** 2)\n",
    "    \n",
    "    if distance <= R:\n",
    "        print(\"YES\")\n",
    "    else:\n",
    "        print(\"NO\")\n"
   ]
