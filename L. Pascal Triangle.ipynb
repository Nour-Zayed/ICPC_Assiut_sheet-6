   "source": [
    "x = int(input())\n",
    "\n",
    "triangle = []\n",
    "for i in range(x):\n",
    "    row = [1] * (i + 1)\n",
    "    for j in range(1, i):\n",
    "        row[j] = triangle[i-1][j-1] + triangle[i-1][j]\n",
    "    triangle.append(row)\n",
    "\n",
    "for row in triangle:\n",
    "    print(\" \".join(map(str, row)))\n"
   ]
